ID = ultranova.micro_rocket_roof
Range = 275
IdealRange = 225
Speed = 45

Components : &<../../rocket_silo_heavy/micro_rocket/micro_rocket/micro_rocket.rules>/Components
{
	Sprite : ^/0/Sprite
	{
		Type = Sprite
		RenderBucket = BulletUpper3
	}

	Homing //This is basically to stop the homing from the original components having any effect
	{
		Type = TargetHoming
		TurnVelocity = 0d
		Delay = .1
		AvoidanceRaycastDistance = 100
		VelocityCompensation = 60%
	}

	MediaEffects : ^/0/MediaEffects
	{
		Type = MediaEffects
		MediaEffects
		[
			: &<../../rocket_silo/he_rocket/rocket/particles/rocket_thruster_plume.rules>
			{
				Bucket = BulletUpper3
			}
			&<../../rocket_silo/he_rocket/rocket/particles/rocket_thruster_plume_light.rules>
		]
	}

	Hit : ^/0/Hit
	{
		Type = SimpleHit
		FriendlyShipLowCollisions = false
	}
}
